"""

x=int(input())
for i in range(x):
  i=input()
  x='+-*/'
  y=i[10::]
  a=0
  while y[a] not in x:
    a+=1
  f=float(y[0:a-1])
  l=float(y[a+1::])
  if y[a]=='+':
    print(f+l)
  if y[a]=='-':
    print(f-l)
  if y[a]=='*':
    print(f*l)
  if y[a]=='/':
    print(f/l)
