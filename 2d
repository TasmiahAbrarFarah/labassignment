#D
a=int(input())
for i in range(a):
  x=input()
  b=0
  c=len(x)-1
  f=-1
  while b<=c:
      m=(b+c)//2
      if x[m]=='1':
        f=m
        c=m-1
      else:
        b=m+1
  if f!=-1:
    print(int(f)+1)
  else:
    print(-1)
